@using ParadiseBookers.Extensions.UrlHelpers
@using ParadiseBookers.Extensions.UrlHelpers
@model string

@{
    var size = ViewData.ModelMetadata.AdditionalValues["size"].ToString();
    var defaulturl = ViewData.ModelMetadata.AdditionalValues["defaulturl"].ToString();   
}

<img src="@Url.GetPhotoUrl(Model, size, defaulturl)" alt="" class="img-polaroid"/>
<input type="file" id="File" name="File" />
